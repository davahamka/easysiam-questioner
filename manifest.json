{
  "name": "EASYSIAM - Questioner ",
  "version": "1.0",
  "description": "An extension for check all questioner on SIAM",
  "icons": {
    "128": "./bin/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://siam.ub.ac.id/kuisioner.php*"],
      "js": ["./build/inject.js"]
    }
  ],
  "web_accessible_resources": [
    "build/app.js",
    "build/handler.js",
    "build/interface.js",
    "build/randomizer.js",
    "build/data/review.js"
  ],
  "manifest_version": 2
}
